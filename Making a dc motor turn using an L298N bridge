int motor1pin1 = 2;
int motor1pin2 = 3;

void setup() {
  pinMode(motor1pin1, OUTPUT);
  pinMode(motor1pin2, OUTPUT);

  pinMode(9, OUTPUT);
}

void loop() {
  analogWrite(9, 100); // Allows for variable outputs especially for PWM (analog)

  digitalWrite(motor1pin1, HIGH); // used simply for turning on or off something (digital binary)
  digitalWrite(motor1pin2, LOW);
  
  delay(300);
}
